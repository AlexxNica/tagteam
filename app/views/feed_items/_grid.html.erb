<% feed_items.each_with_index do |feed_item, i| %>
  <% if i % 2 == 0 %>
    <div class="row feed-item-grid">
  <% end %>
  <%= render partial: 'feed_items/grid_item', as: :feed_item, object: feed_item %>
  <% if (i + 1) % 2 == 0 %>
    </div>
  <% end %>
<% end %>
</div>
<div class="text-center">
<%= render partial: 'shared/paginate', object: feed_items, as: :results %>
</div>
