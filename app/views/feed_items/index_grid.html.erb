<% content_for :top_panel do %>
  <%= render partial: 'hub_feeds/top_panel' %>
<% end %>

<% content_for :tabs do %>
  <%= render partial: 'hub_feeds/tabs', locals: { active: 'items' } %>
<% end %>

<% content_for :tab_content do %>
  <%= render partial: 'alter_view', locals: { view: 'grid', results: @feed_items } %>
  <ul class="list-unstyled flush-top">
    <%= render partial: 'grid', object: @feed_items, as: :feed_items %>
  </ul>
<% end %>
