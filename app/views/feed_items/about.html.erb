<% content_for :top_panel do %>
<div class="col-sm-12">
  <h1><%= @feed_item.title %></h1>
  <h4><%= @hub_feed.feed.title %> <%= @feed_item.created_at.strftime('%F') %></h4>
</div>
<% end %>

<% content_for :tabs do %>
  <%= render partial: 'tabs', locals: { active: 'about' } %>
<% end %>

<% content_for :tab_content do %>
  <div class="nicely-padded about-feed-item">
    <%= render partial: 'about' %>
  </div>
<% end %>
