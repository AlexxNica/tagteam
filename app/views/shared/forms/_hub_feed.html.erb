<%= semantic_form_for([@hub,@hub_feed]) do |f| %>
  <%= f.inputs do %>
    <%= f.input :title, :label => 'New title' %>
    <%= f.input :description, :as => :text %>
  <% end %>
  <%= f.actions do %>
    <%= f.action :submit, as: :button, button_html: { class: 'btn btn-primary' } %>
  <% end %>
<% end %>
