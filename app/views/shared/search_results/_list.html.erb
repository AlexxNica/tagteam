<% list.results.each do|r| %>
  <% if r.class == Feed %>
    <%= render :partial => 'shared/search_results/feed', :object => r %>
  <% end %>
  <% if r.class == FeedItem %>
    <%= render :partial => 'shared/search_results/feed_item', :object => r %>
  <% end %>
  <% if r.class == ActsAsTaggableOn::Tag %>
    <%= render :partial => 'shared/search_results/tag', :object => r %>
  <% end %>
  <div class="clear"></div>
<% end %>
