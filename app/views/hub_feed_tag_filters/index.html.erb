<%= documentation('hub-feed-filter-tab') %>
<h3>Feed-wide filters</h3>
<% if @is_owner %>
  <%= link_to 'Add a new tag to all items in this feed', hub_feed_hub_feed_tag_filters_path(@hub_feed), :data_type => 'AddTagFilter', :class => 'add_filter_control hub_feed_tag_filter' %> <br/>
<% end %>
<div class="hub_feed_tag_filters">
  <%= render :partial => 'shared/line_items/hub_feed_tag_filter', :collection => @hub_feed_tag_filters %>
</div>
