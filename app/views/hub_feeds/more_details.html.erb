<tr class="metadata" id="hub_feed_metadata_<%= @hub_feed.id %>">
  <td>
    <span class="updated"> <%= @hub_feed.feed.updated_at.to_s(:long) %></span>
    <span class="items"><span class="ui-silk inline ui-silk-application-view-list"></span> <%= @hub_feed.feed.feed_items.length %> items</span> <%= link_to('RSS', hub_feed_feed_items_path(@hub_feed, :format => :rss), :title => 'An RSS feed with tag filters applied to it.') %> <%= link_to('Atom', hub_feed_feed_items_path(@hub_feed, :format => :atom), :title => 'An Atom feed with tag filters applied to it.') %>
  </td>
  <td>
    <span class="feed_item_tags"><%= raw @hub_feed.latest_tags(10).collect{|t| tag_display(t, :hub => @hub, :hub_feed => @hub_feed) }.join(' ') %></span> 
  </td>
</tr>
