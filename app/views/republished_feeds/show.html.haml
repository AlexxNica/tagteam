- content_for :top_panel do
  = render partial: 'top_panel', locals: { hub: @hub, republished_feed: @republished_feed }
- content_for :tabs do
  = render partial: 'tabs', locals: { active: 'about' }
- content_for :tab_content do
  .nicely-padded
    %h3 Description:
    - unless @republished_feed.description.blank?
      = sanitize @republished_feed.description
    %h3 Owners:
    = @republished_feed.owners.collect{|o| o.username}.join(', ')
